{
    "name": "saicosys/cakephp-installer",
    "description": "CakePHP 5 applications installer",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Saicosys",
            "homepage": "https://github.com/saicosys"
        },
        {
            "name": "Sandeep Kadyan",
            "homepage": "https://github.com/sandeep-kadyan"
        }
    ],
    "require": {
        "php": ">=8.1",
        "symfony/console": "^6.0",
        "symfony/process": "^6.0",
        "symfony/filesystem": "^6.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "cakephp/cakephp-codesniffer": "^5.0",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Saicosys\\Installer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Saicosys\\Installer\\Test\\": "tests/"
        }
    },
    "bin": [
        "bin/cakephp"
    ],
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
